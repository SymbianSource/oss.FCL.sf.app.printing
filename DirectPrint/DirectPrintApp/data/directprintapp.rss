//
//-----------------------------------------------------------------------------
//
RESOURCE DIALOG r_directprint_print_progress_note
    {
    flags = EAknProgressNoteFlags;
    buttons = R_AVKON_SOFTKEYS_CANCEL;
    items =
        {
        DLG_LINE
            {
            type = EAknCtNote;
            id = EDirectPrintCtrlIdPrintProgressNote;
